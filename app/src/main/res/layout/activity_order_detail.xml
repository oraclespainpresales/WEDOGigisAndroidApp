<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".OrderDetail">
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@mipmap/wedoteam"
            android:id="@+id/imageView4"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/imageView3"
            android:layout_marginLeft="112dp" android:layout_marginStart="112dp"/>
    <ImageView
            android:layout_width="116dp"
            android:layout_height="74dp" app:srcCompat="@mipmap/gigipizza" tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="0dp" android:id="@+id/imageView3"/>
    <TextView
            android:text="TextView"
            android:layout_width="260dp"
            android:layout_height="57dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textChangeOrder" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="84dp"
            app:layout_constraintHorizontal_bias="0.211" android:textSize="18sp"/>
    <TextView
            android:text="TextView"
            android:layout_width="296dp"
            android:layout_height="168dp"
            android:id="@+id/detailOrder" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textChangeOrder" android:layout_marginTop="12dp"
            app:layout_constraintHorizontal_bias="0.262" android:textSize="18sp"/>
    <ImageView
            android:layout_width="54dp"
            android:layout_height="48dp" app:srcCompat="@mipmap/visalogo"
            android:id="@+id/imageVISA"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp"
            android:onClick="goVISA" android:layout_marginTop="56dp"
            app:layout_constraintTop_toBottomOf="@+id/detailOrder"/>
    <ImageView
            android:layout_width="64dp"
            android:layout_height="61dp" app:srcCompat="@mipmap/amex"
            android:id="@+id/imageAMEX" app:layout_constraintStart_toEndOf="@+id/imageVISA"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:onClick="goAMEX" android:layout_marginTop="48dp"
            app:layout_constraintTop_toBottomOf="@+id/detailOrder"/>
    <ImageView
            android:layout_width="59dp"
            android:layout_height="71dp" app:srcCompat="@mipmap/mastercard"
            android:id="@+id/imageMC"
            app:layout_constraintStart_toEndOf="@+id/imageAMEX" android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:onClick="goMC" android:layout_marginTop="44dp"
            app:layout_constraintTop_toBottomOf="@+id/detailOrder"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="57dp" app:srcCompat="@mipmap/cash"
            android:id="@+id/imageCASH" app:layout_constraintStart_toEndOf="@+id/imageMC"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            android:onClick="goCASH" android:layout_marginTop="48dp"
            app:layout_constraintTop_toBottomOf="@+id/detailOrder"/>
    <TextView
            android:text="TextView"
            android:layout_width="184dp"
            android:layout_height="71dp"
            android:id="@+id/paymentResult"
            app:layout_constraintTop_toBottomOf="@+id/imageAMEX" android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="76dp"
            android:layout_marginStart="76dp" android:textSize="18sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>